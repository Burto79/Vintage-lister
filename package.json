{
  "name": "vintage-lister",
  "version": "1.0.0",
  "description": "AI-powered vintage item listing tool with Claude extended thinking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "dev": "tsx watch src/index.ts",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node dist/index.js",
    "start:backend": "node dist/server.js",
    "start:prod": "npm run build:backend && node dist/server.js",
    "analyze": "tsx src/cli.ts analyze",
    "list": "tsx src/cli.ts list",
    "auto": "tsx src/cli.ts auto",
    "install:frontend": "cd frontend && npm install"
  },
  "keywords": ["ebay", "claude", "ai", "vintage", "automation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "chokidar": "^4.0.1",
    "dotenv": "^16.4.5",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "sharp": "^0.33.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.2",
    "axios": "^1.7.2",
    "form-data": "^4.0.0",
    "@modelcontextprotocol/sdk": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
